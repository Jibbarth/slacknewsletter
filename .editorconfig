; top-most EditorConfig(editorconfig.org) file
root = true

[*]
end_of_line = LF
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
indent_size = 4

[*.js]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[{.travis.yml,package.json}]
indent_size = 2
